// The Round Table
digraph {
	node [shape=circle]
	start [label=start]
	node [shape=box]
	readImage [label="image=ImRead(filename:$source)"]
	node [shape=box]
	transform [label="result=Rotate(image:$image,angle:$angle)"]
	node [shape=box]
	sleep [label="Sleep(secs:$sleepSecs)"]
	start -> readImage
	readImage -> transform
	transform -> sleep
	sleep -> transform
}
