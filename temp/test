// The Round Table
digraph {
	node [shape=circle]
	start [label=start]
	node [shape=box]
	readImage [label="image=ImRead(filename:$source)"]
	node [shape=box]
	transform [label="result=MorphologyEx(image:$image,operation:$operation,elementType:$elementType,elementSize:$elementSize,iterations:$iterations)"]
	node [shape=box]
	sleep [label="Sleep(secs:$sleepSecs)"]
	start -> readImage
	readImage -> transform
	transform -> sleep
	sleep -> transform
}
